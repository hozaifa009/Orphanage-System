    <?php
        // include_once "SearchDaily.php";
        function DrawTable($Line)
        {
            echo "<tr>";
            $Array=explode("~",$Line);
            for($i=0;$i<count($Array);$i++)
            {
                echo "<td>". $Array[$i]."</td>";
            }
            echo "</tr>";
        }
